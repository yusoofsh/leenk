// https://www.typescriptlang.org/tsconfig
{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "**/*.astro",
    "**/*.ts",
    "**/*.cjs",
    ".eslintrc.cjs",
    ".prettierrc.cjs"
  ],
  "exclude": ["node_modules"]
}
